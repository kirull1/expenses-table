# Google Sheets API credentials
SPREADSHEET_ID=
SERVICE_ACCOUNT_EMAIL=
# Note: In the actual .env file, replace with your service account private key
# The private key should include newlines as '\n'
SERVICE_ACCOUNT_PRIVATE_KEY=

# Authentication credentials
AUTH_USERNAME=admin
AUTH_PASSWORD=password123
# Hashed password for password123 (bcryptjs compatible with fixed salt)
AUTH_PASSWORD_HASH=$2a$10$Dn6LgZsW2g3ZHIJ7qTozeOOdgZ/Qn9qAKw8F.mySkuPfCVMi2GimC

# JWT configuration
JWT_SECRET=your_very_long_and_secure_random_secret_key_here
JWT_EXPIRES_IN=1h

# Security settings
MAX_LOGIN_ATTEMPTS=5
LOGIN_WINDOW_MINUTES=15

# Build timestamp (automatically generated during build)
BUILD_TIMESTAMP=
